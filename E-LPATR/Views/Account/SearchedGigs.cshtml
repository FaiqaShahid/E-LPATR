@*@model IEnumerable<E_LPATR.Models.Profile>*@
@using E_LPATR.Models
@model List<Profile>
@{
    ViewBag.Title = "SearchedGigs";
}

    <div class="container" style="margin-top:50px">
        <ol class="breadcrumb">
            <li><a href="~/Account/Home">Home</a></li>
            <li class="active">Searched Category</li>
        </ol>
        @foreach (var item in Model)
        {
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <a href="~/Teacher/Home"><img src="~/Images/chemistry.jpg" alt="Alternate Text" class="img-responsive" /></a>
                        <h3>@item.Name</h3>
                        Per Hour: @item.PackagePlan.CostPerHour
                        Per Day: @item.PackagePlan.CostPerDay
                        Per 3 Days: @item.PackagePlan.CostPerHour
                        <div class="col-lg-push-8 col-lg-8"><a href="~/Teacher/Profile" class="btn btn-success">View</a></div>
                    </div>
                </div>
            </div>
        }
    </div>